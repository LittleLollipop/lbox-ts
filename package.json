{
  "name": "@saidev/lbox",
  "version": "0.0.3",
  "description": "Tools for extracting logic layers",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "sai",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest-serializer": "^28.0.0",
    "ts-jest": "^29.1.5"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "jest-serializer"
    ]
  }
}
